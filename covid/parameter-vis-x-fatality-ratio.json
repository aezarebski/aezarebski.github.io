{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "data/parameter-estimate-x-fatality-ratio.json"
    },
    "encoding": {
        "y": {
            "field": "title",
            "type": "ordinal",
            "title": "Article"
        },
        "color": {
            "field": "infectionOrCase",
            "type": "nominal"
        }
    },
    "layer": [
        {
            "mark": {
                "type": "point",
                "filled": true
            },
            "encoding": {
                "x": {
                    "field": "pointEstimate",
                    "type": "quantitative",
                    "scale": {"zero": false},
                    "title": "Mean duration symptom onset to death (days)"
                }
            }
        } ,
        {
            "mark": {
                "type": "rule"
            },
            "encoding": {
                "x": {
                    "field": "lowerBound",
                    "type": "quantitative"
                },
                "x2": {
                    "field": "upperBound",
                    "type": "quantitative"
                }
          }
      }
  ]
}
